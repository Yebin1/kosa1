package DI;

// VO, DTO, DOMAIN 의미를 갖는 클래스
public class NewRecord {

	private int kor;
	private int eng;
	private int path;

	public NewRecord() {
	}

	public NewRecord(int kor, int eng, int path) {
		super();
		this.kor = kor;
		this.eng = eng;
		this.path = path;
	}

	public int getKor() {
		return kor;
	}

	public void setKor(int kor) {
		this.kor = kor;
	}

	public int getEng() {
		return eng;
	}

	public void setEng(int eng) {
		this.eng = eng;
	}

	public int getPath() {
		return path;
	}

	public void setPath(int path) {
		this.path = path;
	}
	
	// 필요에 따라 함수 추가
	

	@Override
	public String toString() {
		return "NewRecord [kor=" + kor + ", eng=" + eng + ", path=" + path + "]";
	}

}
